@Html.Partial("_nav_descarga")
@section sectionHidden{
    @{
        string resultado = string.Empty;
        List<DKbase.web.capaDatos.cCatalogo> lista = WebService.getCatalogosParaDescarga();
        if (lista != null)
        {
            resultado += "<input type=\"hidden\" id=\"hiddenListaCatalogo\" value=\"" + Server.HtmlEncode(Kellerhoff.Codigo.clases.Generales.Serializador.SerializarAJson(lista)) + "\" />";
        }
        WriteLiteral(resultado);
    }
}
@section Scripts{
    @Html.Partial("_scriptsConfig")
}
<div class="row">

    <div class="col-xs-12" id="divGridCatalogo">
    </div>
</div>