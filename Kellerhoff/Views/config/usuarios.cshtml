@Html.Partial("_nav_config")
@section Scripts{
    @Html.Partial("_scriptsConfig")
}
@section sectionHidden{
    @{
        string resultado = string.Empty;
        List<Kellerhoff.Codigo.capaDatos.cUsuario> lista = Kellerhoff.Codigo.clases.AccesoGrilla.GetUsuariosDeCliente("usu_codigo", ((Kellerhoff.Codigo.capaDatos.cClientes)Session["clientesDefault_Cliente"]).cli_codigo, null);
        if (lista != null)
        {
            resultado += "<input type=\"hidden\" id=\"hiddenListaUsuarios\" value=\"" + Server.HtmlEncode(Kellerhoff.Codigo.clases.Generales.Serializador.SerializarAJson(lista)) + "\" />";
        }
        WriteLiteral(resultado);
    }
}
<div class="row">
    <div class="col-lg-2 col-sm-12">
        <div class="clear15 visible-sm visible-xs"></div> <a class="btn_emp" href="#" onclick="usuarioNuevo(); return false;">NUEVO</a>
    </div>
    <div class="col-lg-5 hidden-sm"></div>
    <div class="clear15"></div>
    <div id="divGridUsuarios" class="col-xs-12">
    </div>
</div>