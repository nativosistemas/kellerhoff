@Html.Partial("_nav_secundario")
@Html.Partial("_nav_reservavacunas")

@section Scripts{
    @Html.Partial("_scripts_reservavacunas")
}
@{

    string resultado = string.Empty;

    List<DKbase.web.capaDatos.cReservasVacunasBD> listaReservaVacunas = DKbase.web.acceso.ObtenerReservasVacunas(Boolean.Parse(System.Web.HttpContext.Current.Session["clientes_pages_reservavacunas_SinTroquel"].ToString()));
    if (listaReservaVacunas != null)
    {
        resultado += "<input type=\"hidden\" id=\"hiddenListaReservasVacunas\" value=\"" + Server.HtmlEncode(Kellerhoff.Codigo.clases.Generales.Serializador.SerializarAJson(listaReservaVacunas)) + "\" />";
    }
    WriteLiteral(resultado);
}

